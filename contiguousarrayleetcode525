class Solution {
public:
     int findMaxLength(vector<int>& nums)
      { 
        int res=0;
       for(int i=0;i<nums.size();i++)
       {
        int zeros=0,ones=0;
        for(int j=i;j<nums.size();j++)
        {
            if(nums[j]==0)
            {
             zeros++;
            }
            else
            {
                ones++;
            }
        if(zeros==ones)
        {
          res=max(res,j-i+1);
        }
        }
       
    }
    return res;
    }
    

};
