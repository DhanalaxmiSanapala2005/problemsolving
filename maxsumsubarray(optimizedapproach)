class Solution {
public:
    int maxSubArray(vector<int>& nums)
     {
       int n=nums.size();
       int cursum=nums[0];
       int maxsum=nums[0];
       for(int i=1;i<n;i++)
       {
        cursum=max(nums[i],cursum+nums[i]);
        maxsum=max(cursum,maxsum);
       }
       return maxsum;
    }
};
