#include<bits/stdc++.h>
using namespace std;
void distinctelements(int arr[],int n,int k)
{  vector<int>ans;
      unordered_map<int,int>ump;
     
	int i=0;
	int j=0;
	while(j<n)
	{
		ump[arr[j]]++;
		if(j-i+1==k)
		{
			ans.push_back(ump.size());
			ump[arr[i]]--;
			if(ump[arr[i]]==0)
			{
				ump.erase(arr[i]);
				
			}
			i++;
		}
		j++;
	}
	for(auto x:ans)
	{
		cout<<x<<" ";
	}
}
int main()
{
	int arr[]={1,2,1,3,4,2,3};
	int n=sizeof(arr)/sizeof(arr[0]);
	int k=4;
	distinctelements(arr,n,k);
}
